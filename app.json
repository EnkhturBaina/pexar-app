{
  "expo": {
    "name": "iBalance",
    "slug": "iBalance",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.ibalance.app",
      "buildNumber": "1.0.0"
    },
    "android": {
      "package": "com.ibalance.app",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "923fc7d5-44cf-4fa4-a2cf-3c01a181c1cf"
      }
    },
    "plugins": [
      [
        "expo-updates",
        {
          "username": "etr"
        }
      ]
    ]
  }
}
